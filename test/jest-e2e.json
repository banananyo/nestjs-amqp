{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@dollarsign/nestjs-amqp/(.*)": "<rootDir>/../libs/amqp/src/$1",
    "@dollarsign/nestjs-amqp": "<rootDir>/../libs/amqp/src"
  }
}
